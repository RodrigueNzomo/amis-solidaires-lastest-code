{
  "default": true,
  "MD001": true, // Header levels should only increment by one level at a time
  "MD002": true, // First header should be a h1 header
  "MD003": { "style": "atx" }, // Headers should use atx style (#)
  "MD004": { "style": "consistent" }, // Unordered list style should be consistent
  "MD005": true, // Inconsistent indentation for list items at the same level
  "MD006": false, // Start bulleted lists at the beginning of the line (disabled)
  "MD007": { "indent": 2 }, // List indentation should be 2 spaces
  "MD009": { "br_spaces": 2 }, // Trailing spaces
  "MD010": false, // Hard tabs (disabled)
  "MD012": true, // Multiple consecutive blank lines
  "MD013": false, // Line length (disabled)
  "MD014": false, // Dollar signs used before commands without showing output (disabled)
  "MD018": true, // No space after hash on atx style header
  "MD019": true, // Multiple spaces after hash on atx style header
  "MD022": true, // Headers should be surrounded by blank lines
  "MD023": true, // Headers must start at the beginning of the line
  "MD024": { "siblings_only": true }, // Multiple headers with the same content
  "MD025": { "level": 1 }, // Multiple top-level headers in the same document
  "MD026": { "punctuation": ".,;:!" }, // Trailing punctuation in header
  "MD029": { "style": "ordered" }, // Ordered list item prefix
  "MD030": true, // Spaces after list markers
  "MD031": true, // Fenced code blocks should be surrounded by blank lines
  "MD032": true, // Lists should be surrounded by blank lines
  "MD033": false, // Inline HTML (disabled)
  "MD036": true, // Emphasis used instead of a header
  "MD037": true, // Spaces inside emphasis markers
  "MD038": true, // Spaces inside code span elements
  "MD040": true, // Fenced code blocks should have a language specified
  "MD041": true, // First line in a file should be a top-level header
  "MD046": { "style": "fenced" } // Code block style
}
